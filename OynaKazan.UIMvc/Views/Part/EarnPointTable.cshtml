@model IEnumerable<OynaKazan.UIMvc.Models.UserPanelModels.KazanilanPuanGecmisiModeli>

<div class="card mb-3">
    <div class="card-header">
        <i class="fa fa-table"></i> Kazanılan Puan Geçmişi
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Puan Adedi</th>
                        <th>İşlem Tarihi</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Puan Adedi</th>
                        <th>İşlem Tarihi</th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.PuanAdedi Adet</td>
                            <td>@item.IslemTarihi</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="card-footer small text-muted">@DateTime.Now tarihinde güncellendi.</div>
</div>

